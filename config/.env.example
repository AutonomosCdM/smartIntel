# watsonx Configuration
WATSONX_API_KEY=your_api_key_here
WATSONX_PROJECT_ID=your_project_id_here
WATSONX_SPACE_ID=your_space_id_here
WATSONX_URL=https://us-south.ml.cloud.ibm.com
WATSONX_ORCHESTRATE_API_KEY=your_orchestrate_key_here

# Document Extraction Configuration (for PDF processing pipeline)
DOC_EXTRACTION_MODEL=meta-llama/llama-3-2-90b-vision-instruct
DOC_EXTRACTION_MIN_CONFIDENCE=0.75
ENABLE_HUMAN_REVIEW=false

# Model Configuration
WATSONX_MODEL_ID=ibm/granite-4.0-instruct
WATSONX_TEMPERATURE=0.7
WATSONX_MAX_TOKENS=1024

# External APIs
FRED_API_KEY=your_fred_api_key_here

# Application Settings
ENV=development
LOG_LEVEL=INFO
DEBUG=false

# Database (if needed)
DATABASE_URL=sqlite:///data/procure_genius.db

# Security
SECRET_KEY=your_secret_key_here
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Feature Flags
ENABLE_PREDICTIVE_MODEL=true
ENABLE_AGENT_LOGGING=true
ENABLE_CACHE=true
